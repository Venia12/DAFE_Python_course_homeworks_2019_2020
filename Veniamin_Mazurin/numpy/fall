%matplotlib inline
import matplotlib.pyplot as plt
import numpy as np
def fall(h,a,v0,n,dt):
    t0 = 0
    y=[]
    v=[]
    t=[t0]
    y.append(h)
    v.append(v0)
    
    for i in range(1, n):
        t.append(t[i-1] + dt)
        v.append (v[i-1] + a*dt)
        y.append (y[i-1] + dt*v[i-1])
    
    return (t, y, v)

t, y, v = fall(10, -9.8, 0,100, 0.01 )

plt.plot(t, y)
plt.show()
